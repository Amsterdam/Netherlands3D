{
  "name": "nl.netherlands3d",
  "displayName": "Netherlands3D",
  "version": "3.3.0",
  "unity": "2022.2",
  "description": "A library of Unity3D systems/prefabs meant for reuse/addition that were initially developed for 3D Amsterdam and 3D Utrecht.",
  "type": "library",
  "keywords": [
    "3d",
    "3dtiles",
    "geojson",
    "cityjson",
    "digital twin",
    "twin city",
    "geoservice"
  ],
  "license": "EUPL-1.2",
  "documentationUrl": "https://github.com/Amsterdam/Netherlands3D/tree/main/PackageUserManual/Dutch",
  "changelogUrl": "https://github.com/Amsterdam/Netherlands3D",
  "licensesUrl": "https://github.com/Amsterdam/Netherlands3D/blob/main/LICENSE.txt",
  "changelogURL": "https://github.com/Amsterdam/Netherlands3D/blob/main/CHANGELOG.md",
  "samples": [
    {
      "displayName": "Address Search",
      "description": "Example of a search input field which can be used to search for an address in a selected city",
      "path": "AddressSearch/Samples~/AddressSearch"
    },
    {
      "displayName": "Cameras",
      "description": "Example of a camera based on the one used in 3DAmsterdam and 3DUtrecht, with dragging, scroll zooming etc.",
      "path": "Cameras/Samples~/Cameras"
    },
    {
      "displayName": "BAG-information",
      "description": "Example of showing pdok.nl BAG information in a canvas after clicking on buildings",
      "path": "BAGInformation/Samples~/BAGInformation"
    },
    {
      "displayName": "File Importer",
      "description": "For importing files.",
      "path": "File Importer/Samples~/Scene Setup"
    },
    {
      "displayName": "Import OBJ",
      "description": "For importing OBJ-files.",
      "path": "ModelParsing/Samples~/Import OBJ-File"
    },
    {
      "displayName": "Minimap Template",
      "description": "Template example of how to use the minimap",
      "path": "Minimap/Samples~/Minimap"
    },
    {
      "displayName": "Selection Tools",
      "description": "Example for selecting areas using pointer",
      "path": "SelectionTools/Samples~/SelectionTools"
    },
    {
      "displayName": "Profile Tool",
      "description": "Example showing the profile rendering tool and the DXF or SVG exporting of selected profiles.",
      "path": "ProfileRendering/Samples~/ProfileRendering"
    },
    {
      "displayName": "Sun/Shadows",
      "description": "Example of a Directional Light with shadows based on the position of the sun according to the location,date and time.",
      "path": "Sun/Samples~/Sun"
    },
    {
      "displayName": "Subobject-filtering",
      "description": "Example showing filtering out subobjects based on a buildyear slider",
      "path": "SubobjectFiltering/Samples~/SubobjectFiltering"
    },
    {
      "displayName": "T3DPipeline/CityJSON visualizer Sample",
      "description": "CityJSON visualizer, gets a CityJSON building from a BAGID, visualises it, allows for annotations, and resumbits it to be registered",
      "path": "T3DPipeline/Samples~/"
    },
    {
      "displayName": "TileSystem",
      "description": "Basic TileSystem",
      "path": "Core/Samples~"
    },
    {
      "displayName": "3DTiles",
      "description": "3DTiles tile system example",
      "path": "3DTiles/Samples~"
    },
    {
      "displayName": "Timeline UI Mockup",
      "description": "Ready to use timeline UI prefab",
      "path": "Timeline/Samples~"
    },
    {
      "displayName": "Traffic Sample",
      "description": "Ready to use traffic example",
      "path": "Traffic/Samples~/Traffic Example"
    },
    {
      "displayName": "WMS Texture Projection",
      "description": "TileHandler layer and supporting scripts for projecting WMS textures onto geometry",
      "path": "WMSTextureProjection/Samples~"
    },
    {
      "displayName": "Web Service Reader",
      "description": "Web Service Reader to read WMS' and WFS' from urls",
      "path": "WebServiceReader/Samples~/WebServiceReaderSamples"
    },
    {
      "displayName": "WMS-url",
      "description": "Runtime tooling for correct wms-url's for use in the WMS-Layer",
      "path": "getWMSUrl/GeoService/Samples~/WMSSelector"
    },
    {
      "displayName": "Snapshots",
      "description": "A system that allows you to create snapshots independent of you screen resolution, with or without UI",
      "path": "Snapshots/Samples~/"
    },
    {
      "displayName": "WebGL template example",
      "description": "A WebGL template example to use for Netherlands3D WebGL projects with unityInstance reference and responsive full browser window behaviour.",
      "path": "WebGL/Samples~/WebGL"
    },
    {
      "displayName": "Message System",
      "description": "A simple message system to quickly put up message on screen through code.",
      "path": "MessageSystem/Samples~/"
    }
  ],
  "hideInEditor": false,
  "author": {
    "name": "3D Amsterdam Team",
    "url": "https://3d.amsterdam.nl"
  },
  "dependencies": {
    "com.unity.textmeshpro": "3.0.6",
    "com.unity.inputsystem": "1.3.0",
    "com.unity.render-pipelines.universal": "1.3.0",
    "com.unity.nuget.newtonsoft-json": "3.1.0",
    "com.unity.meshopt.decompress": "0.1.0-preview.5",
    "eu.netherlands3d.coordinates": "1.1.0",
    "eu.netherlands3d.snapshots": "1.0.0",
    "eu.netherlands3d.sun": "1.0.0",
    "eu.netherlands3d.periodic-snapshots": "1.0.0",
    "eu.netherlands3d.filebrowser": "1.3.1",
    "eu.netherlands3d.obj-importer": "1.1.0",
    "eu.netherlands3d.poly2tri": "2.0.0",
    "eu.netherlands3d.selection-tools": "1.0.1",
    "eu.netherlands3d.web-cursors": "1.0.2",
    "eu.netherlands3d.masking": "1.0.2",
    "eu.netherlands3d.simplejson": "1.0.0",
    "eu.netherlands3d.address-search": "1.0.0",
    "eu.netherlands3d.geojson": "1.1.0"
  }
}
